@model IEnumerable<Fiore.Models.Order>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                model.UserId
            </th>
            <th>
                OrderDate
            </th>
            <th>
                TotalPrice
            </th>
            <th>
                HouseNumber
            </th>
            <th>
                StreetName
            </th>
            <th>
                CityName
            </th>
            <th>
                CountryName
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    item.UserId
                </td>
                <td>
                    @item.OrderDate
                </td>
                <td>
                    @item.TotalPrice
                </td>
                <td>
                    @item.HouseNumber
                </td>
                <td>
                    @item.StreetName
                </td>
                <td>
                    @item.CityName
                </td>
                <td>
                    @item.CountryName
                </td>
                <td>
                    <a asp-action="Details" asp-route-id="@item.OrderId">Details</a>
                </td>
            </tr>
        }
    </tbody>
</table>
